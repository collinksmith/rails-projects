<table>
  <thead>
    <tr>
      <% albums.first.attributes.keys.each do |col| %>
        <th><%= col %></th>
      <% end %>
    </tr>
  </thead>

  <tbody>
    <% albums.each do |album| %>
      <tr>
        <% album.attributes.values.each do |value| %>
          <td><%= value %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>