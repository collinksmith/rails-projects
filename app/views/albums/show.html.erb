<%= link_to "Back to band", band_url(@album.band_id) %>
<h1><%= @album.name %></h1>
<%= button_to "Edit Album", edit_album_url(@album), method: :get %>
<%= button_to "Delete Album", album_url(@album), method: :delete %>
<%= button_to "Create New Track", new_album_track_url(@album), method: :get %>

<%= render "album_tracks", album: @album %>